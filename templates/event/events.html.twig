{% extends 'base.html.twig' %}

{% block title %}Les evenements{% endblock %}

{% block body %}

<table>
   <tr>
       <th>Nom</th>
       <th>Date</th>
   </tr>




{% for event in events %}
   <tr>
       <td>{{ event.Name }}</td>
       <td>{{ event.Date |date("m/d/Y") }}</td>
   </tr>
{% endfor %}


</table>

{% endblock %}
